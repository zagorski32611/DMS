<%= render 'layouts/header' %>

<h1> All Repair Orders </h1>
<h3><%= link_to "Create New Repair Order", new_repair_order_url %>&nbsp;</h3>


<table class="box">
  <% @repair_order.each do |ro| %>
  <tr>
    <td><%= ro.window_tag %></td><br/>
    <td><%= ro.model %></td><br/>
    <td><%= ro.make %></td><br/>
    <td><%= ro.color %></td><br/>
    <td><%= ro.miles %></td><br/>

  <% end %>
</table>
